// Обновляет доступ служебного пользователя к API.
//
Процедура ОбновитьДоступСлужебногоПользователяКAPI(Пользователь, Включить) Экспорт
	
	Роли = РолиСлужебногоПользователяИнтеграции();
	
	Если Включить Тогда
		УстановитьРолиСлужебногоПользователя(Пользователь, Роли);
	Иначе
		СнятьРолиСлужебногоПользователя(Пользователь, Роли);
	КонецЕсли;
	
